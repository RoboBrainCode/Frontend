<script type=text/javascript>
    $(function() {
          $('#calculate').bind('click', function() 
          {
            $("#wheel").addClass("spinning");
            $.getJSON('http://127.0.0.1:8000/raquel/rachQuery', 
            {
                query: document.getElementById('hello').value
            }, function(data) 
                {
                     $("#wheel").removeClass("spinning");
                     document.getElementById('hello').value = "";

                     console.log(data);
                     jsonVar=data;
                      jsonStr = JSON.stringify(jsonVar),
                      regeStr = '',
                      f = {
                              brace: 0
                          }; // for tracking matches, in particular the curly braces

                      // document.getElementById('jsonStr').innerHTML = jsonStr;

                      regeStr = jsonStr.replace(/({|}[,]*|[^{}:]+:[^{}:,]*[,{]*)/g, function (m, p1) {
                          var rtnFn = function() {
                                  return '<div style="text-indent: ' + (f['brace'] * 20) + 'px;">' + p1 + '</div>';
                              },
                              rtnStr = 0;
                          if (p1.lastIndexOf('{') === (p1.length - 1)) {
                              rtnStr = rtnFn();
                              f['brace'] += 1;
                          } else if (p1.indexOf('}') === 0) {
                              f['brace'] -= 1;
                              rtnStr = rtnFn();
                          } else {
                              rtnStr = rtnFn();
                          }
                          return rtnStr;
                      });

                  document.getElementById('regeStr').innerHTML = regeStr;



                     return false;
                });
        });
    });     
</script>
